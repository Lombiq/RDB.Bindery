@{
    // The number of page number links to show, 0 means no link, 1 means only the current page, or more accepted.
    Model.Quantity = 4;

    Model.PreviousText = Html.Raw("&larr; Newer");
    Model.NextText = Html.Raw("Older &rarr;");
    Model.Classes.Add("group");
    Model.Metadata.Alternates.Clear();
    Model.Metadata.Type = "Pager_Links";
}

@Display(Model)